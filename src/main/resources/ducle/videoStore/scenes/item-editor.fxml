             <Label text="Title" GridPane.rowIndex="1" />
             <Label text="Rental Type" GridPane.rowIndex="2" />
             <Label text="Genre" GridPane.rowIndex="3" />
             <Label text="Loan Type" GridPane.rowIndex="4" />
